                    android:layout_marginStart="4dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="16dp"
                    android:contentDescription="@string/Weekly_or_Yearly"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/miles"
                    app:layout_constraintTop_toBottomOf="@+id/divider"
                    app:layout_constraintVertical_bias="0.0" />

                <EditText
                    android:id="@+id/milesNum"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="8dp"
                    android:contentDescription="@null"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="number"
                    android:textAlignment="center"
                    app:layout_constraintStart_toEndOf="@+id/milesQuestion"
                    app:layout_constraintTop_toBottomOf="@+id/divider" />

                <EditText
                    android:id="@+id/mileage"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="8dp"
                    android:contentDescription="@null"
                    android:ems="10"
                    android:importantForAutofill="no"
                    android:inputType="number"
                    android:textAlignment="center"
                    app:layout_constraintStart_toEndOf="@+id/mileageQuestion"
